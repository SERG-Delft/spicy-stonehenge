<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://atlantis.st.ewi.tudelft.nl/tudelft/v1/types"
	elementFormDefault="qualified"
	targetNamespace="http://atlantis.st.ewi.tudelft.nl/tudelft/v1/types"
	version="1.0.0">
	<xs:complexType name="OrderData">
		<xs:annotation>
			<xs:documentation />
		</xs:annotation>
		<xs:sequence>
			<xs:element name="accountID" type="xs:int" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="holdingID" type="xs:int" minOccurs="0" maxOccurs="1"></xs:element>
			
			<xs:element name="orderID" type="xs:int" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="orderType" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="orderStatus" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="openDate" type="xs:dateTime" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="completionDate" type="xs:dateTime" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="quantity" type="xs:double" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="price" type="xs:double" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="orderFee" type="xs:double" minOccurs="1" maxOccurs="1"></xs:element>
		    <xs:element name="symbol" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>

